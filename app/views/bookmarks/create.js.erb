var collapsable = document.querySelector("#collapsable-<%= @bookmark.category.id %>");
collapsable.insertAdjacentHTML("afterbegin", "<%= escape_javascript( render 'bookmarks/components/bookmark/bookmark_card', bookmark: @bookmark ) %>");
document.querySelector("#details").innerHTML = "";

<% if !@bookmark.category.is_empty? %>
  var warning = collapsable.querySelector(".empty-category-warning");
  if (warning) {
    warning.remove();
  }
<% end %>
